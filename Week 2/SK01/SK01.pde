size(600, 400);              // Size of screen. Must be the first executable statement
background(255, 255, 0);     // Background color
stroke(0, 0, 255);           // Color of line (RGB)
strokeWeight(10);            // Width of line
line(50, 50, 50, 150);       // Starting pos of line, end pos of line (x, y, x, y)
fill(255, 0, 0);             // Fill color of circle (RGB)
circle(300, 200, 100);       // Circle centered at (x, y, r)
strokeWeight(2);             // Width of line
fill(0, 255, 0);             // Fill color (RGB)
ellipse(300, 200, 300, 20);  // Ellipse centered at (x, y, w, h)
rectMode(CENTER);            // Use center of rectangle for pos instead of top left corner
stroke(255, 0, 0);           // Color of line (RGB)
rect(300, 305, 200, 100);    // Rectangle (x, y, w, h)
